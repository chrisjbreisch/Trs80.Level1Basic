10 INPUT "HORIZONTAL STARTING POINT (0 TO 127)";X
20 INPUT "VERTICAL STARTING POINT (0 TO 47)";Y
30 INPUT "LENGTH OF EACH SIDE (IN BLOCKS) -- (0 TO 47)";K
40 CLS
50 FOR L = X TO X+K*2+1
60   SET(L,Y)
70   SET(L,Y+K+1)
80 NEXT L
90 FOR M = Y TO Y+K+1
100   SET(X,M)
110   SET(X+K*2+1,M)
120 NEXT M
999 GOTO 999